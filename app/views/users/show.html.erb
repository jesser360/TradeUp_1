<h1>Welcome to your profile!</h1>
<%  if(@user.image)%>
<h4>Image:<%=image_tag(@user.image)%></h4>
<%end%>
<h4>Username:<%=@user.username%></h4>
<h4>Email:<%=@user.email%></h4>
<h4>Location:<%=@user.location%></h4>
<h4>Rating:<%=@user.rating%></h4>
<h4>Role:<%=@user.role%></h4>
<h4>Background color:<%=@user.background_color%></h4>
<br />
<h2>My items</h2>
<%@user.items.each do |item|%>
<h5>title:<%=item.title%></h5>
<h5>descrip:<%item.description%></h5>
<br /><br />
<%end%>
<%=link_to 'Edit Profile', edit_user_path_url(@user)%>
<%= link_to 'Create an item', new_item_path_url%>
