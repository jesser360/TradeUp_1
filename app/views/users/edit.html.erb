<div class="container">
      <%=form_for @user, :url => "/users/update/"+(@user.id.to_s), :method => 'patch' do |f| %>
      <%= f.label :username%>
      <%= f.text_field :username%>
      <br />
      <%= f.label :email%>
      <%= f.text_field :email%>
      <br />
      <%= f.label :location%>
      <%= f.text_field :location%>
      <br />
      <%= f.label :Background_Color%>
      <%= f.text_field :background_color%>
      <br />
      <%= f.label :image%>
      <%= f.text_field :image%>
      <%= f.submit "Submit"%>
    <%end %>
  </div>
